    "@babel/preset-typescript": "^7.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.22.0",
    "@types/joi": "^17.2.3",
