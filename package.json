{
  "name": "api-vendas",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/shared/http/server.js",
    "lint-fix": "eslint . --ext .ts --fix",
    "lint": "yarn lint-fix &&  eslint . --ext .ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "build": "rimraf build && babel src --extensions \".js,.ts\" --out-dir build --copy-files",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/shared/http/server.ts",
    "up": "yarn typeorm migration:run",
    "revert": "yarn typeorm migration:revert",
    "drop:migration": "yarn typeorm schema:drop",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test -- --watch",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^27",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^17.0.32",
    "@types/nodemailer": "^6.4.1",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "jest": "^27",
    "jest-mock-extended": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.4",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "ioredis": "^4.24.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1",
    "pg": "^8.5.1",
    "rate-limiter-flexible": "^2.2.1",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "touch-cli": "^0.0.1",
    "typeorm": "^0.2.31",
    "typeorm-pagination": "^2.0.1",
    "uuid": "^8.3.2"
  }
}
